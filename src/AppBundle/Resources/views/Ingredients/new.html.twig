{% extends 'AppBundle:Main:base.html.twig' %}

{% block javascripts %}
    <script type="text/javascript" async src="{{ asset('js/lib.js?%s') | format(random(998)) }}"></script>
{% endblock %}

{% block body %}

    <div class="all">

        {% include('AppBundle:Templates:header.html.twig') %}

        <div class="main">

            <div class="left">

                {{ form_start(form) }}

                    {{ form_row(form.title) }}
                    {{ form_row(form.description) }}
                    {{ form_row(form.brochure) }}

                {{ form_end(form) }}

            </div>
            <div class="right">

            </div>

        </div>

    </div>

{% endblock body %}
